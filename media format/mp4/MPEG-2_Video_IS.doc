\par}\pard\plain \qj\sb60\sa60\nowidctlpar\widctlpar\adjustright \fs20\lang2057\cgrid {Each motion vector component, }{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{], shall be calculated by any process that is equivalent to the following one.  Note that the motion vector predictors shall also be updated by this process.
\par
\par}\pard\plain \s27\qj\li720\keep\keepn\nowidctlpar\widctlpar\tx1080\tx1440\tx1800\tx2160\tx2520\tx2880\tx3240\tx3600\tx3960\tx4320\tx4680\tx5040\tx5400\tx5760\tx6120\tx6480\tx6840\tx7200\tx7560\tx7920\adjustright \fs20\lang2057\cgrid {\i r_size}{ = }{\i f_code}{[}{\i s}{][}{\i t}{] - 1
\par}{\i f}{ = 1 << }{\i r_size}{
\par}{\i high}{ = ( 16 * }{\i f}{ ) -  1;
\par}{\i low}{ = ( (-16) * }{\i f}{ );
\par}{\i range }{= ( 32 * }{\i f}{ );
\par
\par if ( (}{\i f}{  == 1) || (motion_code[r][s][t] == 0) )
\par\tab }{\i delta}{ = motion_code[r][s][t] ;
\par else \{
\par\tab }{\i delta}{ = ( ( }{\i Abs}{(motion_code[r][s][t]) - 1 ) * f ) + motion_residual[r][s][t] + 1;
\par\tab if (motion_code[r][s][t] < 0)
\par\tab \tab }{\i delta}{ = - }{\i delta}{;
\par \}
\par
\par}{\i prediction}{ = }{\i PMV}{[}{\i r}{][}{\i s}{][}{\i t}{];
\par if ( (mv_format == \ldblquote field\rdblquote ) && (}{\i t}{==1) && (picture_structure == \ldblquote Frame picture\rdblquote ) )
\par\tab }{\i prediction}{ = }{\i PMV}{[}{\i r}{][}{\i s}{][}{\i t}{] DIV 2;
\par
\par}{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{]= }{\i prediction}{ + }{\i delta}{;
\par if (}{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{] < }{\i low}{ )
\par\tab }{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{] = }{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{] + }{\i range}{;
\par if (}{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{] > }{\i high}{)
\par}\pard \s27\qj\li720\keep\keepn\nowidctlpar\widctlpar\tx1080\tx1440\tx1800\tx2160\tx2520\tx2880\tx3240\tx3600\tx3960\tx4320\tx4680\tx5040\tx5400\tx5760\tx6120\tx6480\tx6840\tx7200\tx7560\tx7920\adjustright {\tab }{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{] = }{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{] - }{\i range}{;
\par}\pard \s27\qj\li720\keep\keepn\nowidctlpar\widctlpar\tx1080\tx1440\tx1800\tx2160\tx2520\tx2880\tx3240\tx3600\tx3960\tx4320\tx4680\tx5040\tx5400\tx5760\tx6120\tx6480\tx6840\tx7200\tx7560\tx7920\adjustright {
\par if ( (mv_format == \ldblquote field\rdblquote ) && (}{\i t}{==1) && (picture_structure == \ldblquote Frame picture\rdblquote ) )
\par\tab }{\i PMV}{[}{\i r}{][}{\i s}{][}{\i t}{] = }{\i vector\rquote }{[}{\i r}{][}{\i s}{][}{\i t}{] * 2;
